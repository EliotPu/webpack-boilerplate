{
  "name": "webpack-boilerplate",
  "version": "0.1.0",
  "description": "A webpack 5 boilerplate for building web apps (SPA/MPA) with Babel, CSS Modules",
  "browser": "./src/index.mjs",
  "type": "module",
  "scripts": {
    "start": "npm run env:prod npm run build:cross-env && npm run static-server",
    "develop": "npm run env:dev npm run serve:cross-env",
    "build:prod": "npm run env:prod npm run env:build npm run build:cross-env",
    "build:dev": "npm run env:dev npm run env:build npm run build:cross-env",
    "build:cross-env": "cross-env-shell \"webpack -c webpack/webpack.common.cjs -c webpack/webpack.$CONFIG.cjs -m\"",
    "env:prod": "cross-env NODE_ENV=production CONFIG=\"prod\"",
    "env:dev": "cross-env NODE_ENV=development CONFIG=\"dev\"",
    "env:build": "cross-env NODE_BUILD=true",
    "serve:cross-env": "cross-env-shell \"webpack serve -c webpack/webpack.common.cjs -c webpack/webpack.$CONFIG.cjs -m\"",
    "static-server": "static-server ./dist",
    "lint": "eslint . --ext js,mjs,cjs",
    "lint:fix": "eslint . --fix --ext js,mjs,cjs",
    "clean": "rimraf ./dist",
    "test:jest": "cross-env NODE_ICU_DATA=node_modules/full-icu jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EliotPu/webpack-boilerplate.git"
  },
  "keywords": [
    "webpack-boilerplate",
    "webpack",
